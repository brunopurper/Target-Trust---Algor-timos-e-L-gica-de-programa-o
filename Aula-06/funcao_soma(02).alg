algoritmo "funcao_soma"

var
dado1, dado2:inteiro
funcao soma(num1,num2:inteiro):inteiro
var
resultado:inteiro
inicio

      resultado<- num1+num2
      retorne resultado
fimfuncao

inicio

escreval("Digite numero 1")
leia(dado1)
escreval("Digite numero 2")
leia(dado2)

Escreval(soma(dado1,dado2))



fimalgoritmo
