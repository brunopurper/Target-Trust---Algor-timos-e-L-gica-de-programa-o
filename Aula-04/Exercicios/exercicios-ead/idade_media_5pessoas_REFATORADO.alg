algoritmo "idade_5_pessoas_soma_idade"

var
cont:inteiro
soma:inteiro
idade:inteiro

inicio
 cont <- 1
enquanto (cont < 6) faca
Escreval("Pessoa ",cont," digite sua idade: ")
leia(idade)

soma <- soma + idade
cont <- cont + 1

fimenquanto
Escreva("A soma das idades é de: ",soma," anos.")



fimalgoritmo
