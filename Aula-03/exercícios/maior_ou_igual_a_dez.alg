algoritmo "maior_que_dez"

var
numero:inteiro
inicio

Escreval("Digite um número inteiro: ")
leia(numero)

Se (numero > 10) entao
   Escreval("O numero",numero," é maior do que 10")
Senao
   Se (numero < 10) entao
      Escreval ("O numero ",numero," é menor do que 10")
   Senao
      Escreval ("Seu número é igual a 10")
   Fimse
Fimse
fimalgoritmo

